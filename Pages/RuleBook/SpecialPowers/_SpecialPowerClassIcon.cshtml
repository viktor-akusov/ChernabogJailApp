@model ChernabogJailApp.RuleBook.CommonEnums.EnumClass

@switch(Model){
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Wizard:
        <i class="fas fa-hat-wizard"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Cleric:
        <i class="fa-solid fa-hands-praying"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Thief:
        <i class="fa-solid fa-mask"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Warrior:
        <i class="fa-solid fa-shield-halved"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Enlightened:
        <i class="fa-solid fa-om"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Inheritor:
        <i class="fa-solid fa-hurricane"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.LastPrince:
        <i class="fa-solid fa-crown"></i>
        break;
    case ChernabogJailApp.RuleBook.CommonEnums.EnumClass.Heretic:
        <i class="fa-solid fa-skull"></i>
        break;
}
